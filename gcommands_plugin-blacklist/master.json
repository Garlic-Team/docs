, false); // unblacklisted\n```\n\nIf you have inhibitors in command, you need add `new BlacklistInhibitor();`","path":"README.md"}}}},"classes":[{"name":"BlacklistInhibitor","extends":["Inhibitor"],"construct":{"name":"constructor","params":[{"name":"options","optional":true,"type":[[["InhibitorOptions"]]]}],"returns":[[["BlacklistInhibitor"]]]},"props":[{"name":"ephemeral","readonly":true,"type":[[["boolean"]]],"meta":{"line":9,"file":"Inhibitor.d.ts","path":"node_modules/gcommands/dist/inhibitors"}},{"name":"message","readonly":true,"type":[[["string"," | ("],["ctx",": "],["CommandContext","<"],["CacheType","> | "],["ComponentContext","<"],["CacheType",">) => "],["string"]]],"meta":{"line":8,"file":"Inhibitor.d.ts","path":"node_modules/gcommands/dist/inhibitors"}}],"methods":[{"name":"resolveMessage","params":[{"name":"ctx","optional":false,"type":[[["CommandContext","<"],["CacheType","> | "],["ComponentContext","<"],["CacheType",">"]]]}],"returns":[[["string"," | "],["void"]]],"meta":{"line":11,"file":"Inhibitor.d.ts","path":"node_modules/gcommands/dist/inhibitors"}},{"name":"run","params":[{"name":"ctx","optional":false,"type":[[["CommandContext","<"],["CacheType","> | "],["ComponentContext","<"],["CacheType",">"]]]}],"returns":[[["Promise","<"],["any",">"]]],"meta":{"line":5,"file":"BlacklistInhibitor.ts","path":"src/inhibitors"}}],"meta":{"line":4,"file":"BlacklistInhibitor.ts","path":"src/inhibitors"}},{"name":"BlacklistManagerClass","construct":{"name":"constructor","returns":[[["BlacklistManagerClass"]]],"meta":{"line":7,"file":"BlacklistManager.ts","path":"src/utils"}},"props":[{"name":"cache","type":[[["LimitedCollection","<"],["string",", "],["boolean",">"]]],"meta":{"line":5,"file":"BlacklistManager.ts","path":"src/utils"}}],"methods":[{"name":"_getBlacklist","access":"private","params":[{"name":"client","optional":false,"type":[[["GClient","<"],["boolean",">"]]]},{"name":"userId","optional":false,"type":[[["string"]]]}],"returns":[[["Promise","<"],["b